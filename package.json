{
  "name": "telegram-bot",
  "version": "0.2.1",
  "description": "With this bot you won't skip the moment when your favorite movies are available on the torrents!",
  "author": "Dmitrii Baranov",
  "scripts": {
    "start": "npm run watch-debug",
    "build": "npm run build-ts && npm run tslint",
    "serve": "NODE_ENV=production node dist/app.js",
    "watch-node": "nodemon dist/app.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\"  \"npm run watch-ts\" \"npm run watch-node\"",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/app.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "_moduleAliases": {
    "@services": "dist/services",
    "@utils": "dist/utils",
    "@middlewares": "dist/middlewares",
    "@configs": "dist/configs",
    "@constants": "dist/constants",
    "@types": "dist/types"
  },
  "dependencies": {
    "async": "^3.2.0",
    "axios": "^0.21.1",
    "cheerio": "^1.0.0-rc.2",
    "dotenv": "^6.1.0",
    "lodash": "^4.17.11",
    "module-alias": "^2.2.2",
    "mongoose": "^5.3.12",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "string-similarity": "^3.0.0",
    "telegraf": "^3.25.0",
    "telegraf-i18n": "^6.4.0",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@types/async": "^3.2.6",
    "@types/cheerio": "^0.22.10",
    "@types/dotenv": "^6.1.0",
    "@types/lodash": "^4.14.119",
    "@types/node": "^10.12.7",
    "@types/request-promise": "^4.1.42",
    "@types/string-similarity": "^3.0.0",
    "concurrently": "^4.0.1",
    "nodemon": "^1.18.6",
    "prettier": "^2.2.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-consistent-codestyle": "^1.16.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-microsoft-contrib": "^6.2.0",
    "typescript": "^3.1.6"
  }
}